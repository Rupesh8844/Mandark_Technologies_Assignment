package com.Mandark_Technologies;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;
import java.util.List;

public class Problem3 {

	public static void main(String[] args) {
		
		List<String> list = new ArrayList<>();
		list.add("d34");
		list.add("g54");
		list.add("d12");
		list.add("b87");
		list.add("g1");
		list.add("c65");
		list.add("g40");
		list.add("g5");
		list.add("d77");

	   Collections.sort(list,(str1,str2)-> str1.charAt(0) - str2.charAt(0));
	   
	   String[] strArr = {"d34", "g54", "d12", "b87", "g1", "c65", "g40", "g5", "d77"};
	   
	   Arrays.sort(strArr, (str1, str2) -> str1.charAt(0) - str2.charAt(0));
	   
	   
	   String[] st = list.toArray();
	   
	   System.out.println("L1: ");
		
		
	}

}
